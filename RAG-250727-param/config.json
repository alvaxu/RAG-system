{
  "api": {
    "dashscope_api_key": "",
    "mineru_api_key": ""
  },
  "paths": {
    "pdf_dir": "./document/pdf",
    "output_dir": "./document/md",
    "md_dir": "./document/md",
    "central_images_dir": "./central/images",

    "add_pdf_dir": "./document/add_pdf",
    "add_output_dir": "./document/add_md",
    "add_md_dir": "./document/add_md",
    "vector_db_dir": "./central/vector_db",  
    "web_app_dir": "./web_app",
    "memory_db_dir": "./central/memory_db"
  },
  "processing": {
    "chunk_size": 1000,
    "chunk_overlap": 200,
    "max_table_rows": 100,
    "enable_logging": true,
    "enable_smart_filtering": false,
    "semantic_similarity_threshold": 0.1,
    "content_relevance_threshold": 0.01,
    "max_filtered_results": 5
  },
  "vector_store": {
    "vector_dimension": 1536,
    "similarity_top_k": 5,
    "similarity_threshold": 0.1,
    "enable_reranking": true,
    "reranking_method": "hybrid",
    "semantic_weight": 0.7,
    "keyword_weight": 0.3,
    "min_similarity_threshold": 0.001,
    "text_embedding_model": "text-embedding-v1",
    "image_embedding_model": "multimodal-embedding-v1",
    "allow_dangerous_deserialization": true
  },
  "qa_system": {
    "model_name": "qwen-turbo",
    "temperature": 0.5,
    "max_tokens": 1500,
    "enable_sources_filtering": true,
            "min_relevance_score": 0.001,
    "enable_keyword_matching": true,
    "enable_image_id_matching": true,
    "enable_similarity_filtering": true
  },
  "memory": {
    "memory_enabled": true,
    "memory_max_size": 10
  },
  "image_processing": {
    "enable_enhancement": true,
    "enable_enhanced_description_vectorization": true,
    "enhancement_model": "qwen-vl-plus",
    "enhancement_max_tokens": 1000,
    "enhancement_temperature": 0.1,
    "enhancement_batch_size": 5,
    "enable_progress_logging": true,
    "depth_processing_markers": [
      "基础视觉描述:",
      "内容理解描述:",
      "数据趋势描述:",
      "语义特征描述:",
      "chart_type:",
      "data_points:",
      "trends:",
      "key_insights:"
    ]
  },
  "preset_questions": {
    "preset_questions_file": "./preset_questions.json"
  }
}