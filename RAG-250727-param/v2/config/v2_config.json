{
  "system_name": "RAG-System-V2.0.0",
  "version": "2.0.0",
  "debug_mode": false,
  "query_timeout": 60.0,
  "cache_size": 1000,
  "enable_image_search": true,
  "enable_text_search": true,
  "enable_table_search": true,
  "enable_hybrid_search": true,
  "reranking_engine": {
    "enabled": true,
    "name": "dashscope_reranking",
    "version": "2.0.0",
    "debug": false,
    "model_name": "gte-rerank-v2",
    "top_k": 10,
    "similarity_threshold": 0.7,
    "weight_semantic": 0.8,
    "weight_keyword": 0.2,
    "timeout": 30.0,
    "enable_reranking": true,
    "max_batch_size": 10
  },
  "llm_engine": {
    "enabled": true,
    "name": "dashscope_llm",
    "version": "2.0.0",
    "debug": false,
    "model_name": "qwen-turbo",
    "temperature": 0.7,
    "max_tokens": 2000,
    "top_p": 0.8,
    "enable_stream": false,
    "timeout": 60.0,
    "system_prompt": "你是一个专业的AI助手，能够基于提供的上下文信息生成准确、相关、完整的答案。",
    "enable_llm": true
  },
  "smart_filter_engine": {
    "enabled": false,
    "name": "smart_filter",
    "version": "2.0.0",
    "debug": false,
    "enable_filtering": false,
    "similarity_threshold": 0.05,
    "keyword_weight": 0.3,
    "semantic_weight": 0.5,
    "content_quality_weight": 0.2,
    "min_content_length": 10,
    "max_content_length": 10000,
    "enable_keyword_extraction": false,
    "enable_content_quality_check": false,
    "enable_adaptive_threshold": false
  },
  "source_filter_engine": {
    "enabled": true,
    "name": "source_filter",
    "version": "2.0.0",
    "debug": false,
    "enable_filtering": true,
    "relevance_threshold": 0.1,
    "content_overlap_threshold": 0.1,
    "keyword_match_weight": 0.4,
    "semantic_similarity_weight": 0.4,
    "content_quality_weight": 0.2,
    "enable_dynamic_threshold": true,
    "min_sources_to_keep": 1,
    "max_sources_to_keep": 10,
    "enable_source_ranking": true
  },
  "image_engine": {
    "enabled": true,
    "name": "image_engine",
    "version": "2.0.0",
    "debug": false,
    "max_results": 20,
    "timeout": 30.0,
    "cache_enabled": true,
    "cache_ttl": 300,
    "image_similarity_threshold": 0.15,
    "keyword_weight": 0.4,
    "caption_weight": 0.3,
    "description_weight": 0.3,
    "enable_fuzzy_match": true,
    "enable_semantic_search": true
  },
  "text_engine": {
    "enabled": true,
    "name": "text_engine",
    "version": "2.0.0",
    "debug": false,
    "max_results": 10,
    "timeout": 30.0,
    "cache_enabled": true,
    "cache_ttl": 300,
    "text_similarity_threshold": 0.7,
    "keyword_weight": 0.3,
    "semantic_weight": 0.4,
    "vector_weight": 0.3,
    "enable_semantic_search": true,
    "enable_vector_search": true,
    
            "min_required_results": 20,
    
    "recall_strategy": {
      "layer1_vector_search": {
        "enabled": true,
        "top_k": 50,
        "similarity_threshold": 0.3,
        "description": "第一层：向量相似度搜索（主要策略）"
      },
      "layer2_semantic_keyword": {
        "enabled": true,
        "top_k": 40,
        "match_threshold": 0.25,
        "description": "第二层：语义关键词搜索（补充策略）"
      },
      "layer3_hybrid_search": {
        "enabled": true,
        "top_k": 35,
        "vector_weight": 0.4,
        "keyword_weight": 0.3,
        "semantic_weight": 0.3,
        "description": "第三层：混合搜索策略（融合策略）"
      },
      "layer4_smart_fuzzy": {
        "enabled": true,
        "top_k": 30,
        "fuzzy_threshold": 0.2,
        "description": "第四层：智能模糊匹配（容错策略）"
      },
      "layer5_expansion": {
        "enabled": true,
        "top_k": 25,
        "activation_threshold": 20,
        "description": "第五层：智能扩展召回（兜底策略）"
      }
    },
    
    "reranking": {
      "enable_type_specific_optimization": true,
      "type_specific_reranking": {
        "text_analysis": {
          "content_length_weight": 0.15,
          "structure_weight": 0.10,
          "vocabulary_weight": 0.10,
          "professional_terms_weight": 0.05
        },
        "semantic_consistency": {
          "word_overlap_weight": 0.6,
          "word_order_weight": 0.4
        },
        "context_relevance": {
          "word_frequency_weight": 0.4,
          "context_coherence_weight": 0.3,
          "domain_relevance_weight": 0.3
        }
      }
    },
    
    "scoring": {
      "comprehensive_ranking": {
        "base_score_weight": 0.4,
        "content_relevance_weight": 0.3,
        "document_quality_weight": 0.2,
        "search_strategy_weight": 0.1
      },
      "strategy_weights": {
        "vector_similarity": 1.0,
        "hybrid_search": 0.9,
        "semantic_similarity": 0.8,
        "semantic_keyword": 0.7,
        "smart_fuzzy": 0.6,
        "expansion_synonym_expansion": 0.5,
        "expansion_concept_expansion": 0.5,
        "expansion_related_word": 0.5,
        "expansion_domain_expansion": 0.4
      }
    }
  },
  "table_engine": {
    "enabled": true,
    "name": "table_engine",
    "version": "2.0.0",
    "debug": false,
    "max_results": 15,
    "timeout": 30.0,
    "cache_enabled": true,
    "cache_ttl": 300,
    "table_similarity_threshold": 0.65,
    "header_weight": 0.4,
    "content_weight": 0.4,
    "structure_weight": 0.2,
    "enable_structure_search": true,
    "enable_content_search": true
  },
  "hybrid_engine": {
    "enabled": true,
    "name": "hybrid_engine",
    "version": "2.0.0",
    "debug": false,
    "max_results": 25,
    "timeout": 30.0,
    "cache_enabled": true,
    "cache_ttl": 300,
    "image_weight": 0.3,
    "text_weight": 0.4,
    "table_weight": 0.3,
    "enable_cross_search": true,
    "enable_ranking": true,
    "enable_optimization_pipeline": true,
    "optimization_pipeline": {
      "enable_reranking": true,
      "enable_smart_filtering": false,
      "enable_llm_generation": true,
      "enable_intelligent_post_processing": false,
      "enable_source_filtering": true,
      "pipeline_order": [
        "reranking",
        "llm_generation",
        "source_filtering"
      ],
      "intelligent_post_processing": {
        "enable_image_filtering": false,
        "enable_text_filtering": false,
        "enable_table_filtering": false,
        "max_images_to_keep": 5,
        "max_texts_to_keep": 5,
        "max_tables_to_keep": 3,
        "keyword_match_threshold": 0.2
      }
    }
  }
}
